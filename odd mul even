#include<stdio.h>
int input(int e[100], int m)
{
    int i,o=0,ev=0;
    for (i=0;i<m;i++)
    {
        //printf("enter the number [%d]:",i+1);
        scanf("%d ",&e[i]);
        if(e[i]%2==0)
        {
            ev=ev+e[i];
        }
        else
        {
            o=o+e[i];
        }
    }
    //printf("even:[%d]\todd:[%d]\n:",ev,o);
    printf("%d",ev*o);
}
int main()
{
    int a,e[100],i;
    //printf("enter the array dimensions:");
    scanf("%d",&a);
    input(e,a);
    
}
