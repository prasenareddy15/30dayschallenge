#include<stdio.h>
#include<math.h>
int main()
{
    int n,i,c=0,p=0;
    //printf("enter the number :");
    scanf("%d",&n);
    while(n!=0)
    {
        //printf("c:%d\tp:%d\tn:%d\n",c,p,n);
        c=c+(n%10)*pow(2,p);
        n=n/10;
        p=p+1;
        //printf("c:%d\tp:%d\tn:%d\n",c,p,n);
    }
    printf("%d",c);
    
    
}
